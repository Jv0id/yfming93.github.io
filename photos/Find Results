   42: function fixSidebar() {
   43:     var sidebarWrap = document.querySelector('.sidebar-wrap')
   44      window.onscroll = function() {
   45:         var sidebarWrapTop = sidebarWrap.getBoundingClientRect().top
   46:         if (sidebarWrapTop < 21) {
   47:             sidebarWrap.classList.add('fixed')
   48          }
   49          var scrollTop = Math.max(document.documentElement.scrollTop, document.body.scrollTop)
   50          if (scrollTop < 78) {
   51:             sidebarWrap.classList.remove('fixed')
   52          }
