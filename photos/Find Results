 2873      &lt;span class=&quot;hljs-class&quot;&gt;&lt;span class=&quot;hljs-keyword&quot;&gt;@implementation&lt;/span&gt; &lt;span class=&quot;hljs-title&quot;&gt;B&lt;/span&gt;&lt;/span&gt;
 2874      - (&lt;span class=&quot;hljs-keyword&quot;&gt;void&lt;/span&gt;)bar {
 2875:         [&lt;span class=&quot;hljs-keyword&quot;&gt;self&lt;/span&gt;&lt;span class=&quot;hljs-variable&quot;&gt;.a&lt;/span&gt; foo]; &lt;span class=&quot;hljs-comment&quot;&gt;// calling protected method foo from outside A and its subclasses&lt;/span&gt;
 2876      }
 2877      &lt;span class=&quot;hljs-keyword&quot;&gt;@end&lt;/span&gt;&lt;/code&gt;&lt;/pre&gt;

